{
  "cesp_version": "1.0",
  "name": "qianyu-chen-pack",
  "display_name": "陈千语的声音包",
  "version": "1.0.0",
  "categories": {
    "session.start": {
      "sounds": [
        { "file": "sounds/new/1.mp3", "label": "Hello!" }
      ]
    },
    "task.acknowledge": {
      "sounds": [
        { "file": "sounds/work/1.mp3", "label": "交给我" },
        { "file": "sounds/work/2.mp3", "label": "交给我吧" },
        { "file": "sounds/work/3.mp3", "label": "交给我就对了" },
        { "file": "sounds/work/4.mp3", "label": "收到收到管理员你就放100个心吧" }
      ]
    },
    "input.required": {
      "sounds": [
        { "file": "sounds/request-approval/1.mp3", "label": "快用这个" }
      ]
    },
    "task.error": {
      "sounds": [
        { "file": "sounds/error/1.mp3", "label": "可恶" }
      ]
    },
    "task.complete": {
      "sounds": [
        { "file": "sounds/complete/1.mp3", "label": "这次轻松" }
      ]
    },
    "resource.limit": {
      "sounds": [
        { "file": "sounds/limit/1.mp3", "label": "歇歇" }
      ]
    }
  }
}
